<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="MenuButtonsStyle">
        <item name="fab_size">mini</item>
        <item name="fab_showAnimation">@anim/jump_from_down</item>
        <item name="fab_hideAnimation">@anim/jump_to_down</item>
        <item name="fab_shadowColor">#444</item>
        <item name="fab_colorNormal">#FFB805</item>
        <item name="fab_colorPressed">#F2AB00</item>
        <item name="fab_colorRipple">#D99200</item>
    </style>

    <style name="MenuButtonsSmall">
        <item name="fab_size">mini</item>
        <item name="fab_colorNormal">#1565C0</item>
        <item name="fab_colorPressed">#2272CD</item>
        <item name="fab_colorRipple">#62B2FF</item>
    </style>

    <style name="MenuButtonsMid">
        <item name="fab_size">normal</item>
        <item name="fab_colorNormal">#1565C0</item>
        <item name="fab_colorPressed">#2272CD</item>
        <item name="fab_colorRipple">#62B2FF</item>
    </style>

    <style name="MenuButtonsSmall.Green">
        <item name="fab_colorNormal">#43A047</item>
        <item name="fab_colorPressed">#2E7D32</item>
        <item name="fab_colorRipple">#1B5E20</item>
    </style>

    <style name="MenuButtonsSmall.Red">
        <item name="fab_size">mini</item>
        <item name="fab_colorNormal">@color/Friebrick3</item>
        <item name="fab_colorPressed">@color/Friebrick2</item>
        <item name="fab_colorRipple">@color/Friebrick1</item>
    </style>

    <style name="MenuButtonsMid.Red">
        <item name="fab_size">normal</item>
        <item name="fab_colorNormal">@color/Friebrick3</item>
        <item name="fab_colorPressed">@color/Friebrick2</item>
        <item name="fab_colorRipple">@color/Friebrick1</item>
    </style>

    <style name="MenuButtonsSmall.Yellow">
        <item name="fab_size">mini</item>
        <item name="fab_colorNormal">@color/Yellow3</item>
        <item name="fab_colorPressed">@color/Yellow2</item>
        <item name="fab_colorRipple">@color/Yellow1</item>
    </style>

    <style name="MenuButtonsSmall.Orange">
        <item name="fab_size">mini</item>
        <item name="fab_colorNormal">@color/Orange3</item>
        <item name="fab_colorPressed">@color/Orange2</item>
        <item name="fab_colorRipple">@color/Orange1</item>
    </style>

    <style name="MenuButtonsSmall.Grey">
        <item name="fab_size">mini</item>
        <item name="fab_colorNormal">@color/dark_gray</item>
        <item name="fab_colorPressed">@color/light_gray_inactive_icon</item>
        <item name="fab_colorRipple">@color/light_gray</item>
    </style>

    <style name="DrawerItemTextAppearance">
        <item name="android:fontFamily" tools:ignore="NewApi">sans-serif-light</item>
    </style>
</resources>